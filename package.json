{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "generate": "yarn plop --plopfile ./generators/plopfile.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
