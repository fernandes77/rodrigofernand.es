    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "globby": "11.0.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "2.3.0",
    "sitemap": "7.0.0"
  }
