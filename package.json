    "date-fns": "^2.28.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "gray-matter": "^4.0.3",
    "next": "10.2.0",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.26.0",
    "next-slicezone": "^0.1.3",
    "nextjs-progressbar": "^0.0.13",
    "prismjs": "^1.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "6.0.0",
    "react-masonry-css": "^1.0.16",
